{
  "id": "cookie_blocker_rules",
  "name": "Cookie Banner Blocking Rules",
  "version": "1.0",
  "rules": [
    {
      "id": 1,
      "priority": 100,
      "action": {
        "type": "block"
      },
      "condition": {
        "urlFilter": "*",
        "resourceTypes": ["script"],
        "regexFilter": ".*(cookie|consent|gdpr|privacy).*\.(js|json)$"
      }
    }
  ]
}